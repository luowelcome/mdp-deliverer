<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lowell.mdp.receiver.facade.mapper.MsgTemplateMapper">



    <select id="getMsgTemplateByCode" parameterType = "String"
            resultType = "com.lowell.mdp.receiver.facade.mapper.MsgTemplateMapper">
      SELECT * FROM message_templates WHERE code = #{code}
    </select>

</mapper>

